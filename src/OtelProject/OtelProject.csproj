    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>af4a07da-f6f2-4d85-b078-7467a786a828</UserSecretsId>
    <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
